<p id="notice"><%= notice %></p>

<h1>Tesses</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tesses.each do |tess| %>
      <tr>
        <td><%= tess.id %></td>
        <td><%= tess.value %></td>
        <td><%= link_to 'Show', tess %></td>
        <td><%= link_to 'Edit', edit_tess_path(tess) %></td>
        <td><%= link_to 'Destroy', tess, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tess', new_tess_path %>
